{
  "name": "zakjakub/chemie-zije-plugin",
  "description": "Plugin for 'CHEMIE Å½IJE!' WordPress website.",
  "type": "wordpress-plugin",
  "license": "proprietary",
  "minimum-stability": "dev",
  "authors": [
    {
      "email": "mail@jakubzak.eu",
      "name": "Jakub ZAK",
      "homepage": "https://jakubzak.cz",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=8.0",
    "wpackio/enqueue": "^3.5",
    "htmlburger/carbon-fields": "^3.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.7",
    "roots/wordpress": "^6.3",
    "automattic/wordbless": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "Zakjakub\\ChemieZijePlugin\\": "src"
    },
    "classmap": [
      ""
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "roots/wordpress-core-installer": true
    }
  },
  "extra": {
    "installer-paths": {
      "vendor/automattic/wordbless/": [
        "automattic/wordbless"
      ]
    },
    "wordpress-install-dir": "wordpress"
  }
}
